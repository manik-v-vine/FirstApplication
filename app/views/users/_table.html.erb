<h2>Users List</h2>

<script src = "https://ajax.googleapis.com/ajax/libs/jquery/3.2.0/jquery.min.js"></script>
<%= javascript_include_tag 'user'%>
<%= javascript_include_tag 'jquery'%>
<body>
 <table class = "table"> 
  <tr>
    <th>First Name</th>
    <th>Last  Name</th>
    <th>Email</th>
    <th>D.O.B</th>
    <th>Active</th>
    <th>Phone no</th>
    <th>Mobile no</th>
    <th>User Operations</th>
  </tr>

<% @users.each do |user|%>
 <tr>
   <td><%= user.first_name%></td>
   <td><%= user.last_name%></td>
   <td><%= user.email%></td>
   <td><%= user.date_of_birth %></td>
   <td><%= user.active %></td>
   <td><%= user.phone_no%></td>
   <td><%= user.mobile_no%></td>
   <td><%= link_to 'Show', user_path(user), method: :get, :class => 'show_button'%>
       <%= link_to 'Edit', edit_user_path(user), method:  :get, :class => 'edit_button' %>
       <%= link_to 'Delete', user_path(user),method: :delete, :class => 'delete_button' %>
   </td>
</tr>
 <% end %>
 </table>
 

 </body>





