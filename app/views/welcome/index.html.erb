<h1>Welcome to rails</h1>

<br>
<%= @controllerMessage%>

<%= link_to "Go to sample",:controller => :welcome, :action => :sample %><br><br>

<%= form_for :user, :url => :welcome_sample do |f| %>
	First Name: <%= f.text_field :first_name %><br><br>
	Last Name: <%= f.text_field :last_name %><br><br>
	Email: <%= f.text_field :email %><br><br>
	D.O.B: <%= f.text_field :date_of_birth %><br><br>
	Phone: <%= f.text_field :phone_no %><br><br>
	Mobile: <%= f.text_field :mobile_no %><br><br>
<%= f.submit "Submit" %>
<% end %>